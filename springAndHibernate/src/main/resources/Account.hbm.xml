<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.chengong.domainObject">

    <class name="Account" table="T_ACCOUNT">
        <id name="accountNumber" column="ACCOUNT_ID">
            <generator class="native"/>
        </id>
        <property name="accountCreationDate" type="timestamp" column="ACCOUNT_CREATION_DATE"/>
        <property name="userName" column="USER_NAME"/>
    </class>

</hibernate-mapping>